<div id="<%= dom_id client %>">
  <tr>
    <th scope="row"><%= client.id %></th>
    <td><%= client.name %></td>
    <td><%= client.age %></td>
    <td><%= client.movies.last %></td>
    <td><%= link_to "Ver cliente", client,class:"btn btn-info" %></td>
    <td><%= link_to "Editar cliente", edit_client_path(client),class:"btn btn-primary" %></td>
    <td><%= button_to "Eliminar cliente", @client,class:"btn btn-danger", method: :delete %></td>
  </tr>
</div>
